<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="BlazorMonaco" Version="3.2.0" />
    <PackageReference Include="ByteSize" Version="2.1.2" />
    <PackageReference Include="GBX.NET" Version="2.0.6-beta2">
      <IsTrimmable>false</IsTrimmable>
    </PackageReference>
    <PackageReference Include="GBX.NET.Imaging.SkiaSharp" Version="1.1.0-beta2" />
    <PackageReference Include="GBX.NET.LZO" Version="2.1.0-beta2" />
    <PackageReference Include="GBX.NET.NewtonsoftJson" Version="1.0.0">
        <IsTrimmable>false</IsTrimmable>
    </PackageReference>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="8.0.7" />
    <PackageReference Include="SkiaSharp.Views.Blazor" Version="2.88.8" />
  </ItemGroup>

</Project>
